{
    "name": "kb2abot",
    "version": "2.0.6",
    "description": "Bot that connected to Facebook Messenger help you to manage members, entertain in any groups!",
    "main": "main/index.js",
    "scripts": {
        "dev": "node --trace-warnings main/deploy --cookiePath bot.json",
        "start": "node main",
        "debug": "node --trace-warnings --inspect-brk main/deploy --cookiePath bot.json",
        "debug-internal": "node --inspect-brk main dev",
        "update": "node scripts/update",
        "login": "node scripts/login",
        "lint:eslint": "eslint --fix",
        "lint:prettier": "prettier --write . --ignore-path .gitignore"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kb2ateam/kb2abot.git"
    },
    "author": "khoakomlem",
    "license": "MIT",
    "dependencies": {
        "adm-zip": "^0.5.9",
        "axios": "^0.24.0",
        "chalk": "^4.1.2",
        "check-internet-connected": "^2.0.5",
        "cheerio": "^1.0.0-rc.10",
        "decache": "^4.6.0",
        "detective": "^5.2.0",
        "facebook-chat-api": "^1.8.0",
        "fast-safe-stringify": "^2.1.1",
        "folder-hash": "^4.0.1",
        "is-running": "^2.1.0",
        "jimp": "^0.16.1",
        "log-to-file": "^3.0.0",
        "minimist": "^1.2.5",
        "node-emoji": "^1.11.0",
        "node-fetch": "^2.6.6",
        "ora": "^5.4.1",
        "recursive-readdir": "^2.2.2",
        "simple-git": "^2.48.0",
        "socket.io-client": "^4.4.0",
        "string-similarity": "^4.0.4",
        "tiktok-scraper": "^1.4.36",
        "uniqid": "^5.4.0",
        "node-fetch": "^2.6.0"
    },
    "devDependencies": {
        "eslint": "^8.3.0",
        "eslint-config-prettier": "^8.3.0",
        "prettier": "^2.5.0"
    },
    "eslintConfig": {
        "env": {
            "commonjs": true,
            "es2021": true,
            "node": true
        },
        "extends": [
            "eslint:recommended",
            "prettier"
        ],
        "parserOptions": {
            "ecmaVersion": 12
        },
        "rules": {
            "indent": [
                "error",
                "tab"
            ],
            "linebreak-style": [
                "error",
                "unix"
            ],
            "quotes": [
                "error",
                "single"
            ]
        },
        "globals": {
            "kb2abot": "writable",
            "loader": "readable",
            "fca": "readable"
        }
    },
    "prettier": {
        "semi": false,
        "singleQuote": true,
        "jsxSingleQuote": true,
        "tabWidth": 4,
        "useTabs": false,
        "trailingComma": "es5",
        "bracketSpacing": true
    },
    "engines": {
        "node": ">=14"
    }
}
